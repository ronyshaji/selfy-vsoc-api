{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://127.0.0.1:8000/ais/deviationUnknown",
  "title": "deviationUnkown",
  "description": "The VSOC also receives information from the AIS. For this, the function ais_deviation_unknown() to get information from the AIS for an unknown deviation. The request is a POST request from the AIS to the VSOC.",
  "type": "object",
  "properties": {
    "extension_type": {
      "description": "property extension from the request (comming from the STIX format)",
      "type": "string"
    },
    "source_vehicle": {
      "description": "VIN of the source vehicle",
      "type": "integer",
      "uniqueItems": true
    },
    "source_ais": {
      "description": "ID of the source AIS",
      "type": "integer",
      "uniqueItems": true
    },
    "source_rsu": {
      "description": "ID of the source RSU",
      "type": "integer",
      "uniqueItems": true
    },
    "observable": {
      "description": "observed data",
      "type": "object"
    }
  },
  "required": [ "extension_type", "source_vehicle", "source_ais", "source_rsu", "observable" ],
  "additionalProperties": true
}